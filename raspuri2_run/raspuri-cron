49 3 * * *  date >> /root/RASPURI/region/OUT/cron.log 2>&1
50 3 * * *  bash /root/RASPURI/bin/raspuriRUN 0 >> /root/RASPURI/region/OUT/cron.log 2>&1
49 15 * * *  date >> /root/RASPURI/region/OUT/cron.log 2>&1
